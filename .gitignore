# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp

# Ignore application configuration
/config/application.yml

# Ignore working js files
/app/assets/javascripts/beerdata.js
/app/assets/javascripts/charts.js
/app/assets/javascripts/conversions.js
/app/assets/javascripts/matrix.js
/app/assets/javascripts/polarstyle.js
/app/assets/javascripts/stackedbar.js